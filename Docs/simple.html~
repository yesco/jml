  '</div>';
  if (pos) {
    var d = document.getElementById(pos);
    d.insertAdjacentHTML("beforebegin", s);
    renumber();
    goto(current);
    goto(current);
    goto(current);
  } else {
    var d = document.getElementById("all");
    d.insertAdjacentHTML("beforeend", s);
    renumber();
    goto(-1);
    goto(-1);
    goto(-1);
  }
  // this fixes multicolumn overview "woles"
  window.resize();
